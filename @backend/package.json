{
  "name": "@control/backend",
  "version": "1.0.1",
  "private": true,
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/@config/orm.config.ts",
    "migrate:create": "npm run typeorm migration:create -- -n",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:run": "npm run typeorm migration:run",
    "migrate:revert": "npm run typeorm migration:revert",
    "migrate:show": "npm run typeorm migration:show",
    "migrate:log": "npm run typeorm schema:log",
    "migrate:clear": "npm run typeorm cache:clear",
    "migrate:version": "npm run typeorm version",
    "seed:create": "npm run typeorm migration:create -c seed -- -n",
    "seed:generate": "npm run typeorm migration:generate -c seed -- -n",
    "seed:run": "npm run typeorm migration:run -c seed",
    "seed:revert": "npm run typeorm migration:revert -c seed",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --config src/config/orm.config.ts"
  },
  "dependencies": {
    "@control/core": "file:../@core",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "asn1js": "^2.0.26",
    "bcrypt": "^3.0.6",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "connect-flash": "^0.1.1",
    "control-consultas-doc": "^1.0.5",
    "date-format": "^2.1.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.0.0",
    "express-session": "^1.16.2",
    "faker": "^4.1.0",
    "helmet": "^3.20.0",
    "joi": "^14.3.1",
    "jsontoxml": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.2.2",
    "module-alias": "^2.2.1",
    "mongodb": "^3.3.0-beta2",
    "mysql": "^2.17.1",
    "node-forge": "^0.10.0",
    "node-webcrypto-ossl": "^1.0.48",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pem-promise": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "soap": "^0.28.0",
    "typegoose": "^5.9.0",
    "typeorm": "^0.2.18",
    "typeorm-seeding": "^1.0.0-rc.3",
    "typeorm-typedi-extensions": "^0.2.3",
    "xadesjs": "^2.0.15",
    "xml-crypto": "^1.4.0",
    "xml-serializer-ts": "^1.0.2",
    "xmldsigjs": "^2.0.25"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/connect-flash": "0.0.35",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.14",
    "@types/faker": "^4.1.5",
    "@types/helmet": "0.0.43",
    "@types/jest": "^23.3.13",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/jszip": "^3.1.6",
    "@types/node": "^10.14.15",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/pem": "^1.9.5",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@fe": "dist/@fe/",
    "@control/config": "dist/@config/",
    "@control/api": "dist/@api/",
    "@control/db": "dist/@database/",
    "@control/common": "dist/@common/"
  }
}
